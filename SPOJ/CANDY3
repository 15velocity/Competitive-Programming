#include<iostream>
using namespace std;

int main()
{
    long long tc,n,a,rem;
    cin>>tc;
    for(long long j=0;j<tc;j++)
    {
        rem=0;
        cin>>n;
        for(long long i=1;i<=n;i++)
        {
            cin>>a;
            rem=rem+a%n;
        }
        if(rem%n==0)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
